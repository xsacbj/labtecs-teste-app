<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activities.MedicalRecordDetailsActivity">

    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/btnBack"
            android:textColor="#fff"
            android:backgroundTint="#21AFFF"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    />
    <TextView
            android:text="Você sente dor?"
            android:layout_width="200dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="25dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView4"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="24sp"/>
    <TextView
            android:text="Sim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView4" app:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/textView4"
            android:id="@+id/textView13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textSize="22sp"
            android:text="Qual intencidade?"/>
    <TextView
            android:text="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView14"
            app:layout_constraintBottom_toBottomOf="@+id/textView13"
            app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="20sp"/>
    <TextView
            android:text="Sente algum desconforto?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:lines="2"
            android:minLines="2"
            android:singleLine="false"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView15"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="24sp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView13"/>
    <TextView
            android:text="Sim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16" app:layout_constraintStart_toEndOf="@+id/textView15"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView15"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"/>
    <TextView
            android:text="Em qual parte do corpo?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView17" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textSize="22sp"/>
    <TextView
            android:text="Cabeça"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18" app:layout_constraintStart_toEndOf="@+id/textView17"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView17"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"/>
    <TextView
            android:text="Passou por alguma cirugia?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView19" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="24sp"/>
    <TextView
            android:text="Sim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20"
            app:layout_constraintBottom_toBottomOf="@+id/textView19"
            app:layout_constraintStart_toEndOf="@+id/textView19" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="20sp"/>
    <TextView
            android:text="Há quanto tempo?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView21" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView19"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textSize="22sp"/>
    <TextView
            android:text="1 a 2 meses"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22" app:layout_constraintStart_toEndOf="@+id/textView21"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView21"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"/>
    <TextView
            android:text="Que tipo de cirugia?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView23" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textSize="22sp"/>
    <TextView
            android:text="Saúde"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView24"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView23"
            app:layout_constraintStart_toEndOf="@+id/textView23" android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"/>
    <TextView
            android:text="Você sentiu alguma dor após a cirurgia?"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView25" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView23" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:textSize="22sp"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="Sim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView26" app:layout_constraintStart_toEndOf="@+id/textView25"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView25"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="20sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>